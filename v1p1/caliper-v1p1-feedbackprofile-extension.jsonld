{
  "@context": ["http://purl.imsglobal.org/ctx/caliper/v1p1", {
    "FeedbackEvent": "caliper:FeedbackEvent",

    "Comment": "caliper:Comment",
    "LikertScale": "caliper:LikertScale",
    "MultiselectionScale": "caliper:MultiselectionScale",
    "NumericScale": "caliper:NumericScale",
    "Rating": "caliper:Rating",
    "Scale": "caliper:Scale",

    "ratingComment": {"@id": "caliper:ratingComment", "type": "@id"},
    "commentedOn": {"@id": "caliper:commentedOn", "type": "@id"},
    "commenter": {"@id": "caliper:commenter", "type": "@id"},
    "itemLabels": {"@id": "caliper:itemLabels", "@container": "@list"},
    "itemValues": {"@id": "caliper:itemValues", "@container": "@list"},
    "rater": {"@id": "caliper:rater", "type": "@id"},
    "rated": {"@id": "caliper:rated", "type": "@id"},
    "scale": {"@id": "caliper:scale", "type": "@id"},
    "selections": {"@id": "caliper:selections", "@container": "@list"},

    "maxLabel": {"@id": "caliper:maxLabel", "type": "xsd:string"},
    "maxSelections": {"@id": "caliper:maxSelections", "type": "xsd:integer"},
    "maxValue": {"@id": "caliper:maxValue", "type": "xsd:decimal"},
    "minLabel": {"@id": "caliper:minLabel", "type": "xsd:string"},
    "minSelections": {"@id": "caliper:minSelections", "type": "xsd:integer"},
    "minValue": {"@id": "caliper:minValue", "type": "xsd:decimal"},
    "orderedSelection": {"@id": "caliper:orderedSelection", "type": "xsd:boolean"},
    "points": {"@id": "caliper:points", "type": "xsd:integer"},
    "question": {"@id": "caliper:question", "type": "xsd:string"},
    "step": {"@id": "caliper:step", "type": "xsd:decimal"},

    "Commented": "caliper:actions/Commented",
    "Ranked": "caliper:actions/Ranked"
  }]
}
